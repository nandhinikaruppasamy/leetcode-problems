class Solution {
    public boolean judgeSquareSum(int c) {
        int num = (int)Math.sqrt(c);
       for(int i=0;i<=num;i++){
        int nu = i*i;
        int min = c-nu;
        int val = (int)Math.sqrt(min);
        if(val*val == min){
            return true;
        }
       }
         return false;
       
    }
}
