class Solution {
    public boolean judgeSquareSum(int c) {
        int num = (int)Math.sqrt(c);
       for(int i=0;i<=num;i++){
        int nu = i*i;
        int min = c-nu;
        double n = Math.sqrt(min);
        int d = (int)n;
        if(n==d){
            return true;
        }
       }
         return false;
       
    }
}
