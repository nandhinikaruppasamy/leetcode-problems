class Solution {
    public String generateTheString(int n) {
        String temp = "";
        if(n % 2 == 0 ){
            for(int i=1;i<=n-1;i++){
                temp += 'a';
            }
            temp += 'b';
        }
        else{
            for(int i=1;i<=n;i++){
                temp += 'a';
            }
        }
        return temp;
    }
}
